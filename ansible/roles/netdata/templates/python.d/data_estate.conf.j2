# Data Estate Collector Configuration
# {{ ansible_managed }}

update_every: {{ netdata_data_estate_update_every | default(60) }}
retries: 3
priority: 90000

local:
    status_json_path: '{{ netdata_data_estate_status_json }}'
    device_status_path: '{{ netdata_data_estate_device_status }}'
    failure_log_path: '{{ netdata_data_estate_failure_log }}'
    markers_dir: '{{ netdata_data_estate_markers_dir }}'
    failure_window_minutes: {{ netdata_data_estate_failure_window_minutes }}

