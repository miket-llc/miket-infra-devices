[Unit]
Description=Nextcloud Database Backup Timer (Nightly before restic)

[Timer]
OnCalendar={{ nextcloud_db_backup_schedule }}
RandomizedDelaySec=60
Persistent=true

[Install]
WantedBy=timers.target

