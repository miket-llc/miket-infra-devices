<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.miket.usersymlinks</string>
    <key>ProgramArguments</key>
    <array>
        <string>{{ ansible_env.HOME }}/.scripts/create_user_symlinks.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>StandardOutPath</key>
    <string>/tmp/com.miket.usersymlinks.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.miket.usersymlinks.err</string>
</dict>
</plist>

